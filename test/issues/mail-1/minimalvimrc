set nocompatible
let &runtimepath = '~/.vim/bundle/vimtex,' . &runtimepath
let &runtimepath .= ',~/.vim/bundle/vimtex/after'
filetype plugin indent on
syntax enable

nnoremap q :qall!<cr>
nnoremap e :echo map(synstack(line('.'), col('.')), 'synIDattr(v:val, "name")')<cr>

silent edit hw9.tex
normal! G
